      <MemberSignature Language="F#" Value="new CoreAnimation.CATransaction : Foundation.NSObjectFlag -&gt; CoreAnimation.CATransaction" Usage="new CoreAnimation.CATransaction t" />
      <MemberType>Constructor</MemberType>
      <AssemblyInfo>
        <AssemblyVersion>0.0.0.0</AssemblyVersion>
        <AssemblyName>Xamarin.iOS</AssemblyName>
      </AssemblyInfo>
      <Attributes>
        <Attribute FrameworkAlternate="xamarin-ios-sdk-12">
