    <Member MemberName="CascadeList">
      <MemberSignature Language="C#" Value="public static readonly MonoTouch.Foundation.NSString CascadeList;" />
      <MemberSignature Language="ILAsm" Value=".field public static initonly class MonoTouch.Foundation.NSString CascadeList" />
      <MemberType>Field</MemberType>
      <AssemblyInfo apistyle="old">
