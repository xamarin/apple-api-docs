      <MemberSignature Language="C#" Value="public AvailabilityAttribute (MonoTouch.ObjCRuntime.Platform introduced, MonoTouch.ObjCRuntime.Platform deprecated = MonoTouch.ObjCRuntime.Platform.None, MonoTouch.ObjCRuntime.Platform obsoleted = MonoTouch.ObjCRuntime.Platform.None, MonoTouch.ObjCRuntime.Platform unavailable = MonoTouch.ObjCRuntime.Platform.None);" />
